{
  "name": "global-time-schedule",
  "dockerComposeFile": ["../../compose.yml"],
  "service": "global-time-schedule",
  "runServices": ["global-time-schedule"],
  "workspaceFolder": "/global-time-schedule",
  "forwardPorts": [5000],
  "postCreateCommand": "cd front && npm install && npm run storybook",
  "remoteUser": "node",
  "customizations": {
    "vscode": {
      "extensions": [
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "Orta.vscode-jest",
        "streetsidesoftware.code-spell-checker",
        "zokugun.explicit-folding"
      ],
      "settings": {
        "workbench.colorCustomizations": {
          "tab.activeBackground": "#3363a3",
          "titleBar.activeBackground": "#3363a3",
          "titleBar.activeForeground": "#000"
        },
        "json.schemaDownload.enable": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      }
    }
  }
}
