{
  "name": "global-time-schedule",
  "dockerComposeFile": ["../../compose.yml"],
  "service": "global-time-schedule",
  "runServices": ["global-time-schedule"],
  "workspaceFolder": "/front",
  "forwardPorts": [5000],
  "postCreateCommand": "npm install && npm run storybook",
  "customizations": {
    "vscode": {
      "extensions": [
        "GitHub.copilot",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "Orta.vscode-jest",
        "streetsidesoftware.code-spell-checker"
      ],
      "settings": {
        "workbench.colorCustomizations": {
          "tab.activeBackground": "#3363a3",
          "titleBar.activeBackground": "#3363a3",
          "titleBar.activeForeground": "#000"
        },
        "json.schemaDownload.enable": true
      }
    }
  }
}
